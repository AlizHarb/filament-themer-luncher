parameters:
    tmpDir: build/phpstan
    level: 5
    paths:
        - src
        - config
        - lang
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Unsafe usage of new static\(\)#'
        - '#Method AlizHarb\\ThemerLuncher\\Models\\Theme::getRows\(\) return type has no value type specified in iterable type array.#'
        - '#Call to an undefined static method AlizHarb\\Themer\\Facades\\Theme::.*#'
